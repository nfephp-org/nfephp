			<xs:element name="xMotivo" type="TMotivo">
				<xs:annotation>
					<xs:documentation>Descrição literal do status da consulta ao lote da CL-e</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="loteCLe" type="TLoteCLe" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="versao" type="TVerCLe" use="required"/>
	</xs:complexType>
